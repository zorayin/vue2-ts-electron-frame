    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "start": "electron .",
    "rebuild": "cd ./node_modules/.bin && electron-rebuild --force --module-dir=../../",
    "report": "vue-cli-service build --report"
  },
